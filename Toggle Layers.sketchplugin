// Toggle Layers (ctrl alt cmd h)

var layercompare = "ğŸ‘€"; // change this to whatever you'd like to match
var layergroups = doc.currentPage().layers();

for (var i=0; i<layergroups.count(); i++) {
	var layergroup = [layergroups objectAtIndex: i];
	var layername = [layergroup name];

	if (layername.match(layercompare)) {
		if ([layergroup isVisible]) {
			[layergroup setIsVisible:false];
		} else {
			[layergroup setIsVisible:true];
		}
	}
}